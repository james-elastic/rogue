#include <stdio.h>
#include <SDL2/SDL.h>

#include "error.h"

void Error::Log(const char *errStr) {
  SDL_LogError(SDL_LOG_CATEGORY_APPLICATION,
	       "%s",
	       errStr);
  return;
}

void Error::Log(int category,
		const char *errStr) {
  SDL_LogError(category,
	       "%s",
	       errStr);
  return;
}
